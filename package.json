{
  "name": "@elonehoo/only-random",
  "version": "0.0.9",
  "description": "Generate random numbers that are consecutively unique.",
  "author": "Elone Hoo <huchengyea@163.com>",
  "license": "MIT",
  "homepage": "https://github.com/elonehoo/only-random#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elonehoo/only-random"
  },
  "bugs": "https://github.com/elonehoo/only-random/issues",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "prepublishOnly": "npm run build",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.5.0",
    "@elonehoo/rimraf": "0.0.1",
    "@types/node": "22.5.4",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "^2.0.5",
    "bumpp": "9.5.2",
    "eslint": "9.10.0",
    "pnpm": "9.10.0",
    "typescript": "5.6.2",
    "unbuild": "^2.0.0",
    "vite": "^5.4.3",
    "vitest": "2.0.5"
  }
}
