{
  "name": "@elonehoo/only-random",
  "version": "0.0.5",
  "description": "Generate random numbers that are consecutively unique.",
  "author": "Elone Hoo <huchengyea@163.com>",
  "license": "MIT",
  "homepage": "https://github.com/elonehoo/only-random#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elonehoo/only-random"
  },
  "bugs": "https://github.com/elonehoo/only-random/issues",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "prepublishOnly": "npm run build",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@elonehoo/rimraf": "0.0.1",
    "@types/node": "18.15.11",
    "@vitest/coverage-c8": "^0.30.1",
    "bumpp": "9.1.0",
    "eslint": "8.38.0",
    "pnpm": "8.2.0",
    "typescript": "5.0.4",
    "unbuild": "^1.2.1",
    "vite": "^4.2.1",
    "vitest": "0.30.1"
  }
}
